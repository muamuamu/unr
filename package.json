{
  "name": "unr",
  "version": "1.0.0",
  "description": "Control the changing state of an object.",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
